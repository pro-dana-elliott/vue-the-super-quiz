{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "rules": {
        "object-curly-spacing":0,
        "indent":0,
		"semi":0,
		"no-multi-spaces":0,
		"comma-spacing":0,
		"eol-last":0,
		"standard/object-curly-even-spacing":0,
        "no-trailing-spaces":0,
        "object-property-newline":0,
        "spaced-comment":0,
        "no-multiple-empty-lines":0,
        "no-tabs":0,
        "key-spacing":0
    }
}
